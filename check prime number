def prime_num(num):
    if num==1:
        print("It is not prime number")
    if num==2:
        print("It is a prime number")
    if num > 2:
        for i in range(2,num):
            if num % i==0:
                print("It is not a prime number")
                break
        else:
            print("It is a prime number")
prime_num(10)

